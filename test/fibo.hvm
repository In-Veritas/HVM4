@add = λ{ #Z: λb.b ; #S: λp.λb. #S{@add(p, b)}; }
@fib = λ{ #Z: #Z{} ; #S: λ{ #Z: #S{#Z{}} ; #S: λp. !P&L=p ; @add(@fib(#S{P₀}), @fib(P₁))}} 

@two = #S{#S{#Z{}}}
@three = #S{#S{#S{#Z{}}}}

@main = @fib(@add(@three, @three))
